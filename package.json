{
  "name": "cering.dev",
  "version": "1.0.0",
  "description": "CERiNG's website.",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf dist && rm -rf .cache",
    "dev": "parcel src/*.pug",
    "push": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose",
    "build": "npm run clear && parcel build src/*.pug --public-url ./ && cp src/CNAME dist/CNAME && npm run push"
  },
  "keywords": [],
  "author": "Spencer Gunning <swcgunning@gmail.com> (https://github.com/aemx)",
  "license": "MIT",
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "pug": "^3.0.2",
    "push-dir": "^0.4.1",
    "stylus": "^0.54.8"
  },
  "dependencies": {}
}
